<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout7"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/grey"
    tools:context=".fragments.WorkoutBuilderFragment" >

    <Button
        android:id="@+id/buttonBackLogin2"
        android:layout_width="38dp"
        android:layout_height="49dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:backgroundTint="@color/teal"
        android:paddingLeft="8dp"
        app:cornerRadius="10dp"
        app:icon="@drawable/back_arrow"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ListView
        android:id="@+id/listViewSongsList"
        android:layout_width="340dp"
        android:layout_height="250dp"
        android:layout_marginTop="16dp"
        android:background="@color/secondary_grey"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <ListView
        android:id="@+id/listViewWorkoutsList"
        android:layout_width="340dp"
        android:layout_height="250dp"
        android:layout_marginTop="16dp"
        android:background="@color/secondary_grey"
        app:layout_constraintEnd_toEndOf="@+id/listViewSongsList"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/listViewSongsList"
        app:layout_constraintTop_toBottomOf="@+id/textViewBuilderIntensity" />

    <TextView
        android:id="@+id/textViewBuilderIntensity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="TextView"
        android:textColor="@color/white"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/listViewWorkoutsList"
        app:layout_constraintStart_toStartOf="@+id/listViewWorkoutsList"
        app:layout_constraintTop_toBottomOf="@+id/listViewSongsList" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="Songs Chosen"
        android:textColor="@color/white"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonCompletedWorkout"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:backgroundTint="@color/teal"
        android:text="Done"
        android:textSize="20sp"
        app:cornerRadius="10sp"
        app:layout_constraintEnd_toEndOf="@+id/listViewWorkoutsList"
        app:layout_constraintStart_toStartOf="@+id/listViewWorkoutsList"
        app:layout_constraintTop_toBottomOf="@+id/listViewWorkoutsList" />

</androidx.constraintlayout.widget.ConstraintLayout>